package optimization;

import java.util.List;


public class GeneticAlgorithmProblem<T> implements OptimizationProblem<T> {

	CrossoverFn crossoverFn;
	public GeneticAlgorithmProblem(EvaluationFunction evalFn, Object mutationFunctions, CrossoverFn crossover) {
		// TODO Auto-generated constructor stub
	}

	@Override
	public Individual<T> createRandom() {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public double value(Individual<T> individual) {
		// TODO Auto-generated method stub
		return 0;
	}
	
	public Individual<T> mate(List<Individual<T>> parents) {
		return crossover.mate(parents.get(0),parents.get(1));
	}

}
