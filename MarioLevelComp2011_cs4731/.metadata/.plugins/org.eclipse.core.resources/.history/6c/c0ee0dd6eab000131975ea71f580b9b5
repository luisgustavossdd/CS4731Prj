package dk.itu.mario.level;

import java.util.ArrayList;

public class GeneticAlgorithm extends OptimizationAlgorithm {

	private int populationSize;
	private int populationToMate;
	private int populationToMutate;
	
	private Individual[] population;
	
	public GeneticAlgorithm(int population, double replacementRate, double mutationRate, OptimizationProblem optimizationProblem) {
		super(optimizationProblem);
		populationSize		= population;
		populationToMate 	= (int) Math.round(replacementRate * population);
		populationToMutate	= (int) Math.round(mutationRate * population);
		
